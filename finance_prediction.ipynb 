   ]
  },
  {
   "cell_type": "code",
   "execution_count": 525,
   "id": "1fcae59a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# use knn imputer to fill value \n",
    "\n",
    "imputer_best = KNNImputer(n_neighbors=6, weights=\"uniform\") \n",
    "\n",
    "X_imputed_val = imputer_best.fit_transform(x_val)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 526,
