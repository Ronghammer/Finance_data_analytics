    "# standardide data\n",
    "\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "x_val_ss= x_val_sm.copy()\n",
    "\n",
    "ss = StandardScaler()\n",
    "\n",
    "x_val_ss= ss.fit_transform(x_val_ss)\n",
    "\n",
    "x_val_ss= pd.DataFrame(x_val_ss)\n",
    "\n",
    "x_val_ss.columns= x_val_sm.columns\n",
    "\n",
    "x_val_ss.head(3)"
   ]
  },
